<web-app>
  <display-name>Aplicação de Música</display-name>

  <servlet>
    <servlet-name>segundaServlet</servlet-name>
    <servlet-class>br.com.testandoServlet.SegundaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>segundaServlet</servlet-name>
    <url-pattern>/segundaServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>criaMusica</servlet-name>
    <servlet-class>br.com.testandoServlet.CriaMusicaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>criaMusica</servlet-name>
    <url-pattern>/criaMusica</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>addMusica</servlet-name>
    <servlet-class>web.MusicaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addMusica</servlet-name>
    <url-pattern>/addMusica</url-pattern>
  </servlet-mapping>

</web-app>
